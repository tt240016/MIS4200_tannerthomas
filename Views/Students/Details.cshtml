@model MIS4200_tt240016.Models.Student

@{
    ViewBag.Title = "Student Details";
}

<h2>Student Details</h2>

<div>
    
    <dl class="dl-horizontal">
        <dt>
            Student First Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.studentFirst)
        </dd>

        <dt>
            Student Last Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.studentLast)
        </dd>

        <dt>
            Student Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.studentEmail)
        </dd>

        <dt>
            Student Phone
        </dt>

        <dd>
            @Html.DisplayFor(model => model.studentPhone)
        </dd>

        <dt>
            Course ID
        </dt>

        <dd>
            @Html.DisplayFor(model => model.courseID)
        </dd>

    </dl>
